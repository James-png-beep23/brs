                  phx-click="switch_other_appl_detail_tab"
                  phx-value-index={index}
                  class={[
                    "inline-block px-1 pb-3 mr-3 text-sm font-medium border-b-2 whitespace-nowrap lg:mr-3 xl:mr-5",
                    @active_other_appl_detail_tab == index && "border-blue-500 text-blue-500",
                    @active_other_appl_detail_tab != index &&
                      "border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300"
                  ]}
                >
                  <%= tab.title %>
                </a>
              <% end %>
